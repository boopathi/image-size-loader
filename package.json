  ],
  "main": "dist/cjs.js",
  "engines": {
    "node": ">=9",
    "npm": ">=5.6"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src -d dist --extensions '.ts' --ignore \"'src/**/interfaces.ts','src/**/*.d.ts','src/**/*.test.ts'\"",
    "clean": "del dist",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint 'src/**/*.{ts,tsx}'",
    "lint:ts": "tsc --noEmit",
    "prebuild": "npm run clean",
    "prepare": "husky install",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.14.0",
    "core-js": "^3.14.0",
    "image-size": "^1.0.0",
    "loader-utils": "^2.0.0",
    "schema-utils": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.4",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
